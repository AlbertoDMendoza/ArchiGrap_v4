@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ag: <http://archigraph.org/ontology#> .
@prefix data: <http://archigraph.org/data#> .

# =============================================================================
# ArchiGraph v4 Sample Data
# Example enterprise architecture data for testing
# =============================================================================

# =============================================================================
# Locations
# =============================================================================

data:loc-headquarters a ag:Location ;
    ag:hasName "Headquarters" ;
    ag:hasDescription "Main corporate headquarters" ;
    rdfs:label "Headquarters" .

data:loc-datacenter a ag:Location ;
    ag:hasName "Primary Data Center" ;
    ag:hasDescription "Main data center hosting critical systems" ;
    ag:partOf data:loc-headquarters ;
    rdfs:label "Primary Data Center" .

data:loc-cloud a ag:Location ;
    ag:hasName "Cloud (AWS)" ;
    ag:hasDescription "Amazon Web Services cloud infrastructure" ;
    rdfs:label "Cloud (AWS)" .

# =============================================================================
# Actors
# =============================================================================

data:actor-it-dept a ag:Actor ;
    ag:hasName "IT Department" ;
    ag:hasDescription "Information Technology department responsible for systems" ;
    ag:locatedAt data:loc-headquarters ;
    rdfs:label "IT Department" .

data:actor-finance a ag:Actor ;
    ag:hasName "Finance Team" ;
    ag:hasDescription "Finance and accounting team" ;
    ag:locatedAt data:loc-headquarters ;
    rdfs:label "Finance Team" .

data:actor-hr a ag:Actor ;
    ag:hasName "HR Department" ;
    ag:hasDescription "Human Resources department" ;
    ag:locatedAt data:loc-headquarters ;
    rdfs:label "HR Department" .

data:actor-customers a ag:Actor ;
    ag:hasName "Customers" ;
    ag:hasDescription "External customers accessing services" ;
    rdfs:label "Customers" .

# =============================================================================
# Capabilities
# =============================================================================

data:cap-financial-mgmt a ag:Capability ;
    ag:hasName "Financial Management" ;
    ag:hasDescription "Capability to manage financial operations and reporting" ;
    ag:hasOwner data:actor-finance ;
    ag:hasStatus "Active" ;
    rdfs:label "Financial Management" .

data:cap-hr-mgmt a ag:Capability ;
    ag:hasName "Human Resource Management" ;
    ag:hasDescription "Capability to manage employee lifecycle and HR operations" ;
    ag:hasOwner data:actor-hr ;
    ag:hasStatus "Active" ;
    rdfs:label "Human Resource Management" .

data:cap-customer-service a ag:Capability ;
    ag:hasName "Customer Service" ;
    ag:hasDescription "Capability to provide support and service to customers" ;
    ag:hasStatus "Active" ;
    rdfs:label "Customer Service" .

data:cap-data-analytics a ag:Capability ;
    ag:hasName "Data Analytics" ;
    ag:hasDescription "Capability to analyze business data and generate insights" ;
    ag:hasOwner data:actor-it-dept ;
    ag:hasStatus "Active" ;
    rdfs:label "Data Analytics" .

# =============================================================================
# Processes
# =============================================================================

data:proc-payroll a ag:Process ;
    ag:hasName "Payroll Processing" ;
    ag:hasDescription "Monthly payroll calculation and payment process" ;
    ag:realizesCapability data:cap-financial-mgmt ;
    ag:hasOwner data:actor-finance ;
    rdfs:label "Payroll Processing" .

data:proc-recruiting a ag:Process ;
    ag:hasName "Recruiting" ;
    ag:hasDescription "Process for hiring new employees" ;
    ag:realizesCapability data:cap-hr-mgmt ;
    ag:hasOwner data:actor-hr ;
    rdfs:label "Recruiting" .

data:proc-support-ticket a ag:Process ;
    ag:hasName "Support Ticket Handling" ;
    ag:hasDescription "Process for handling customer support requests" ;
    ag:realizesCapability data:cap-customer-service ;
    rdfs:label "Support Ticket Handling" .

# =============================================================================
# Systems
# =============================================================================

data:sys-erp a ag:System ;
    ag:hasName "Enterprise ERP" ;
    ag:hasDescription "Core enterprise resource planning system" ;
    ag:supports data:cap-financial-mgmt ;
    ag:supports data:proc-payroll ;
    ag:locatedAt data:loc-datacenter ;
    ag:hasOwner data:actor-it-dept ;
    ag:hasStatus "Active" ;
    ag:isActive true ;
    rdfs:label "Enterprise ERP" .

data:sys-hris a ag:System ;
    ag:hasName "HR Information System" ;
    ag:hasDescription "Human resources information and management system" ;
    ag:supports data:cap-hr-mgmt ;
    ag:supports data:proc-recruiting ;
    ag:dependsOn data:sys-erp ;
    ag:locatedAt data:loc-cloud ;
    ag:hasOwner data:actor-hr ;
    ag:hasStatus "Active" ;
    ag:isActive true ;
    rdfs:label "HR Information System" .

data:sys-crm a ag:System ;
    ag:hasName "Customer CRM" ;
    ag:hasDescription "Customer relationship management system" ;
    ag:supports data:cap-customer-service ;
    ag:supports data:proc-support-ticket ;
    ag:locatedAt data:loc-cloud ;
    ag:hasStatus "Active" ;
    ag:isActive true ;
    rdfs:label "Customer CRM" .

data:sys-datawarehouse a ag:System ;
    ag:hasName "Data Warehouse" ;
    ag:hasDescription "Central data warehouse for analytics" ;
    ag:supports data:cap-data-analytics ;
    ag:dependsOn data:sys-erp ;
    ag:dependsOn data:sys-hris ;
    ag:dependsOn data:sys-crm ;
    ag:locatedAt data:loc-datacenter ;
    ag:hasOwner data:actor-it-dept ;
    ag:hasStatus "Active" ;
    ag:isActive true ;
    rdfs:label "Data Warehouse" .

data:sys-portal a ag:System ;
    ag:hasName "Customer Portal" ;
    ag:hasDescription "Self-service portal for customers" ;
    ag:supports data:cap-customer-service ;
    ag:dependsOn data:sys-crm ;
    ag:locatedAt data:loc-cloud ;
    ag:hasStatus "Active" ;
    ag:isActive true ;
    rdfs:label "Customer Portal" .

# =============================================================================
# Actor-System Relationships (uses)
# =============================================================================

data:actor-finance ag:uses data:sys-erp .
data:actor-hr ag:uses data:sys-hris .
data:actor-hr ag:uses data:sys-erp .
data:actor-it-dept ag:uses data:sys-datawarehouse .
data:actor-customers ag:uses data:sys-portal .

# =============================================================================
# Actor-Process Relationships (performs)
# =============================================================================

data:actor-finance ag:performs data:proc-payroll .
data:actor-hr ag:performs data:proc-recruiting .
